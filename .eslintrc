{
  "extends": "eslint-config-airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2,
    "react/prefer-stateless-function": 0,
    "comma-dangle": 0,
    "indent": 0,
    "no-console": 1,
    "max-len": [ 1, 120 ],
    "no-param-reassign": 0,
    "global-require": 0,
    "no-underscore-dangle": 0,
    "no-alert": 0,

    "react/prop-types": 1,
    "no-restricted-syntax": 1,
    "no-unused-vars": 1,
    "no-debugger": 1
  }
}
